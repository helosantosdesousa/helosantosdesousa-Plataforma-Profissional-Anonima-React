{
  "expo": {
    "name": "Plataforma Profissional An√¥nima",
    "slug": "PlataformaProfissionalAnonima",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ppa_logo.png",
    "scheme": "plataformaprofissionalanonima",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.helosansou.PlataformaProfissionalAnonima"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ppa_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d912d2b8-162a-409b-8e42-69f1c450cee2"
      }
    }
  }
}
